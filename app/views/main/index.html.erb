<div class="jumbotron" id="main-jumbotron">
  <form class="form-inline" role="form" id="main-search" action="/results/search" method="POST">
    <div class="form-group">
      <label class="sr-only" for="search-item">What are you looking for?</label>
      <input type="text" class="form-control" id="search-item" name="itemsearch" placeholder="e.g. pale ales, tacos, sushi">
    </div>
    <span>near</span>
    <div class="form-group">
      <button type="button" class="btn btn-default" id="location-button">
        <span class="glyphicon glyphicon-map-marker" id="location-button-icon"></span>
        <img src="/assets/712.GIF" id="location-button-loading-gif">
      </button>
      <label class="sr-only" for="search-location">Where are you looking for it?</label>
      <input type="text" class="form-control" id="search-location" name="searchbar" placeholder="e.g. Palo Alto, CA">
    </div>
    <button type="submit" class="btn btn-default" id="search-button">
      <span class="glyphicon glyphicon-search"></span>
    </button>
    <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  </form>
  <div id="showcase-item-rating">
    <% for i in (1..4) %>
    <span class="glyphicon glyphicon-star"></span>
    <% end %>
    <div id>Seared Scallops at The French Laundry</div>
  </div>
</div>


<div class="container-fluid">
  <div class="row">
    <div class="col-sm-12">
      <h2 id="currently-popular-heading">Currently Popular Near Palo Alto, CA</h2>
    </div>
  </div>
<% for i in (1..2) %>
  <div class="row popular-items-row">
    <% for i in (1..6) %>
      <div class="col-sm-2">
        <%= render "shared/item_square" %>
      </div>
    <% end %>
  </div>
  <% end %>
</div>
